{
	"name": "react-temp",
	"private": true,
	"version": "0.0.0",
	"type": "module",
	"scripts": {
		"dev": "vite --host",
		"build": "tsc -b && vite build",
		"lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
		"preview": "vite preview",
		"commit": "git-cz"
	},
	"dependencies": {
		"@ant-design/icons": "^5.4.0",
		"react": "^18.3.1",
		"react-dom": "^18.3.1"
	},
	"devDependencies": {
		"@types/node": "^22.0.0",
		"@types/react": "^18.3.3",
		"@types/react-dom": "^18.3.0",
		"@types/react-router-dom": "^5.3.3",
		"@typescript-eslint/eslint-plugin": "^7.15.0",
		"@typescript-eslint/parser": "^7.15.0",
		"@vitejs/plugin-react": "^4.3.1",
		"antd": "^5.19.3",
		"axios": "^1.7.2",
		"classnames": "^2.5.1",
		"commitizen": "^4.3.0",
		"cz-git": "^1.9.4",
		"echarts": "^5.5.1",
		"eslint": "^8.57.0",
		"eslint-config-prettier": "^9.1.0",
		"eslint-plugin-react-hooks": "^4.6.2",
		"eslint-plugin-react-refresh": "^0.4.7",
		"html2canvas": "^1.4.1",
		"konva": "^9.3.14",
		"lint-staged": "^15.2.7",
		"normalize.css": "^8.0.1",
		"prettier": "3.3.3",
		"react-konva": "^18.2.10",
		"react-router-dom": "^6.25.1",
		"sass": "^1.77.8",
		"typescript": "^5.2.2",
		"vite": "^5.3.4",
		"vite-plugin-mock-dev-server": "^1.6.1",
		"zustand": "^4.5.4"
	},
	"config": {
		"commitizen": {
			"path": "node_modules/cz-git"
		}
	},
	"lint-staged": {
		"*.{js,jsx,less,md,json}": [
			"eslint --fix",
			"git add"
		],
		"*.ts?(x)": [
			"eslint --fix",
			"git add"
		]
	},
	"gitHooks": {
		"pre-commit": "lint-staged",
		"commit-msg": "commitlint --edit"
	}
}
